{
  "name": "garage_warden",
  "version": "0.0.1",
  "description": "Controls a garage door as well as rfid/keypad access.",
  "main": "index.js",
  "author": "Scott Olson",
  "private": true,
  "scripts": {
    "db:migrate": "node -r esm node_modules/.bin/sequelize-cli db:migrate",
    "start": "node -r esm ./src/index.js",
    "watch": "NODE_ENV=development nodemon --watch src/ -r esm ./src/index.js"
  },
  "dependencies": {
    "config": "^3.2.2",
    "esm": "^3.2.25",
    "express-winston": "^4.0.1",
    "lodash": "^4.17.19",
    "mqtt": "^3.0.0",
    "nanoid": "^2.1.1",
    "nanoid-dictionary": "^3.0.0",
    "nanotimer": "^0.3.15",
    "rpi-gpio": "^2.1.4",
    "sequelize": "^5.19.0",
    "sqlite3": "^4.1.0",
    "winston": "^3.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-register": "^6.26.0",
    "chalk": "^3.0.0",
    "eslint": "^6.2.2",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.2"
  }
}
